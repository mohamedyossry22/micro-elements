{
  "name": "micro-element",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:element": "npm run build --prod --output-hashing=none --single-bundle=true",
    "build:element:navbar": "npm run build:element -- --project navbar",
    "build:element:appspro": "npm run build:element -- --project appspro",
    "build:element:store": "npm run build:element -- --project store",
    "build:elements": "npm-run-all --parallel build:element:** --sequential build:elements:copy",
    "build:elements:copy": "run-p build:elements:copy:js build:elements:copy:css",
    "build:elements:copy:js": "cpy \"**/*.js\" \"!micro-element/*.js\" \"../src/assets\" --cwd=dist --parents",
    "build:elements:copy:css": "cpy \"**/*.css\" \"!micro-element/*.css\" \"../src/assets\" --cwd=dist --parents",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/elements": "^14.1.3",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "ngx-build-plus": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "cpy-cli": "^3.1.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.2"
  }
}
